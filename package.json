{
  "name": "redux-sharing",
  "version": "1.0.0",
  "description": "The sharing demo site which is implemented by redux",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ignore-path .gitignore --cache --ext .js,.jsx . --fix",
    "test": "mocha --compilers js:babel-register --require ./test/testHelper.js 'test/**/*.@(js|jsx)'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lenchen1112/ReduxSharing.git"
  },
  "keywords": [
    "react",
    "redux",
    "react-redux"
  ],
  "author": "lenchen1112",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lenchen1112/ReduxSharing/issues"
  },
  "homepage": "https://github.com/lenchen1112/ReduxSharing#readme",
  "dependencies": {
    "immutable": "^3.7.6",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.0",
    "redux": "^3.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.6.0",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.6.0",
    "chai": "^3.5.0",
    "chai-immutable": "^1.5.3",
    "eslint": "^2.2.0",
    "eslint-plugin-react": "^4.1.0",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "pre-commit": "^1.1.2",
    "react-addons-test-utils": "^0.14.7",
    "react-hot-loader": "^1.3.0",
    "webpack": "^1.12.14"
  },
  "precommit": [
    "lint",
    "test"
  ],
  "eslintConfig": {
    "rules": {
      "max-len": 0,
      "dot-notation": [
        2,
        {
          "allowKeywords": true
        }
      ]
    },
    "parser": "babel-eslint",
    "plugins": [
      "react"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  }
}
